{
  "expo": {
    "name": "St.johns EM High school",
    "slug": "schoolAssistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logoApp.png",
    "splash": {
      "image": "./logoApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./logoApp.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.avinash_com.schoolAssistant",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./logoApp.png"
    },
    "extra": {
      "eas": {
        "projectId": "87b15f16-ac06-4154-b00f-51638a0b587c"
      }
    }
  }
}