{
  "expo": {
    "name": "St.johns EM High school",
    "slug": "schoolAssistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logoApp.png",
    "splash": {
      "image": "./logoApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "no"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./logoApp.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.StjohnsEM.Highschool",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./logoApp.png"
    },
    "extra": {
      "eas": {
        "projectId": "87b15f16-ac06-4154-b00f-51638a0b587c"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "owner": "avinash_com"
  }
}